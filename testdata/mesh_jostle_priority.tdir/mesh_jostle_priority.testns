$ORIGIN slow.
$TTL 3600

ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id sleep=5
SECTION QUESTION
a	IN	A
SECTION ANSWER
a	IN	A 1.1.1.1
ENTRY_END

ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id sleep=5
SECTION QUESTION
b	IN	A
SECTION ANSWER
b	IN	A 2.2.2.2
ENTRY_END

$ORIGIN fast.

ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id
SECTION QUESTION
c	IN	A
SECTION ANSWER
c	IN	A	10.20.30.40
ENTRY_END

